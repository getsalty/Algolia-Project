{
  "name": "algolia-project",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -c green.bold,blue.bold 'npm:dev:docker' 'npm:dev:next'",
    "dev:docker": "docker compose up",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "db:migrate:dev": "prisma migrate dev",
    "prepare": "husky install && prisma generate"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/bundle-analyzer": "^12.2.3",
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^4.12.0",
    "@trpc/client": "^10.0.0-proxy-beta.21",
    "@trpc/next": "^10.0.0-proxy-beta.21",
    "@trpc/react-query": "^10.0.0-proxy-beta.21",
    "@trpc/server": "^10.0.0-proxy-beta.21",
    "@vercel/analytics": "^1.1.2",
    "algoliasearch": "^4.22.1",
    "material-ripple-effects": "^2.0.1",
    "next": "^12.3.4",
    "next-auth": "4.16.4",
    "node-cache": "^5.1.2",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.19.14",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.2.52",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.1",
    "postcss": "^8.4.14",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.1.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "5.3.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write"
  },
  "prisma": {
    "seed": "ts-node --transpile-only -r tsconfig-paths/register ./prisma/seed.ts"
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "overrides": {
    "react": "18.2.0"
  }
}
